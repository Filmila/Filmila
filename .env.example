# API Configuration
VITE_API_URL=https://api.filmila.com
VITE_API_KEY=your_api_key_here

# Authentication
VITE_AUTH_DOMAIN=filmila.auth0.com
VITE_AUTH_CLIENT_ID=your_auth0_client_id
VITE_AUTH_REDIRECT_URI=https://filmila.netlify.app/callback

# Application Settings
VITE_APP_NAME=Filmila
VITE_APP_ENV=production

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true

# Storage
VITE_STORAGE_BUCKET=filmila-storage
VITE_STORAGE_REGION=us-east-1

# Social Media
VITE_FACEBOOK_APP_ID=your_facebook_app_id
VITE_GOOGLE_CLIENT_ID=your_google_client_id 